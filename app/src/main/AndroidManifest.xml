<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.carlosespejo.wguapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".TermsList"
            android:label="@string/title_activity_add_term"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".CoursesList"
            android:label="Course List" />
        <activity
            android:name=".AssessmentsList"
            android:label="Assessment list" />
        <activity
            android:name=".CourseDetail"
            android:label="Course Details" />
        <activity
            android:name=".TermDetail"
            android:label="Term Details" />
        <activity android:name=".AddTermScreen" />
        <activity
            android:name=".AddCourseScreen"
            android:label="@string/title_activity_add_course_screen" />
        <activity android:name=".AddMentorScreen" />
        <activity android:name=".AddAssessmentScreen" />
        <activity android:name=".MentorDetailScreen" />
        <activity android:name=".AssessmentDetailScreen"></activity>
    </application>

</manifest>